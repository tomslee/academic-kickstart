	note = {arXiv: 1809.04356},
	keywords = {Computer Science - Artificial Intelligence, Computer Science - Machine Learning, Statistics - Machine Learning},
	file = {arXiv\:1809.04356 PDF:C\:\\Users\\tomsl\\Zotero\\storage\\TBAHLIU5\\Fawaz et al. - 2018 - Deep learning for time series classification a re.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\tomsl\\Zotero\\storage\\9XLMAD39\\1809.html:text/html}
}

