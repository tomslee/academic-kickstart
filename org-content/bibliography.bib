	keywords = {Computer Science - Machine Learning, Statistics - Machine Learning},
	file = {arXiv\:1807.06732 PDF:C\:\\Users\\tomsl\\Zotero\\storage\\EE4W6S6U\\Gilmer et al. - 2018 - Motivating the Rules of the Game for Adversarial E.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\tomsl\\Zotero\\storage\\K2R5FGJD\\1807.html:text/html}
