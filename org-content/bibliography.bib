	file = {arXiv\:1803.09010 PDF:C\:\\Users\\tomsl\\Zotero\\storage\\RQJ75C4H\\Gebru et al. - 2018 - Datasheets for Datasets.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\tomsl\\Zotero\\storage\\FRJ75GRM\\1803.html:text/html}
