	keywords = {Inpatients, Collaborative filtering, Data mining, Electronic health records, Practice variability, Prediction models, Decision Support Systems, Clinical, Electronic Health Records, Hospitalization, Humans, Information Storage and Retrieval, Medical Order Entry Systems, Patient Care, Practice Patterns, Physicians'},
