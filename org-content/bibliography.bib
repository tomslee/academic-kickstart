	year = {2017},
	keywords = {assistance sociale, bureaucrates de première ligne, discretion, discrétion, new public management, nouvelle gestion publique, regulatory technology, street-level bureaucrats, technologie de réglementation, welfare},
	pages = {75--95},
	file = {Snapshot:C\:\\Users\\tomsl\\Zotero\\storage\\PIXULV8A\\77904FD4A49A37247D9D9D28C762BCFC.html:text/html}
