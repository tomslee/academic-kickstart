@techreport{alexy_fistful_2010,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {A {Fistful} of {Dollars}: {Financial} {Rewards}, {Payment} {Norms}, and {Motivation} {Crowding} in {Open} {Source} {Software} {Development}},
	shorttitle = {A {Fistful} of {Dollars}},
	url = {http://papers.ssrn.com/abstract=1007689},
	abstract = {Existing literature on open source software (OSS) maintains that intrinsic motivation and extrinsic financial rewards have a unidimensionally positive effect on the motivation of individual developers. Based on self-determination theory, which underlies most of these studies, we challenge this assumption. We argue that the effect of payment on both intrinsic motivation and total motivation of OSS developers is far more complex. To illustrate our point, we introduce the concept of individuals’ norms about payment to the field of OSS. In doing so, we are able to show that payment norms moderate the effect of payment on intrinsic motivation and total motivation. Conducting a scenario experiment, we find that intrinsic motivation decreases for individuals with norms against payment. This effect becomes even stronger when analyzing for mediation effects. Total motivation is impacted positively by payment, but the effect turns insignificant for individuals with norms for payment. Our findings help explain the results of previous studies in which OSS developers did not seem to be affected by motivation crowding. They further contribute to the more general debate on how to manage individuals in the absence of formal contracts. From a practical perspective, we show that financial rewards may create a management dilemma for OSS project leaders.},
	number = {ID 1007689},
	urldate = {2015-05-31},
	institution = {Social Science Research Network},
	author = {Alexy, Oliver and Leitner, Martin},
	month = apr,
	year = {2010},
	keywords = {crowding out, financial rewards, intrinsic motivation, norms about payment, open source software (OSS)},
	file = {Snapshot:C\:\\Users\\tomsl\\Zotero\\storage\\RSFNMN4Q\\papers.html:text/html}
}

