	keywords = {Computer Science - Machine Learning, Statistics - Machine Learning, Computer Science - Information Retrieval},
	file = {arXiv\:1812.02353 PDF:C\:\\Users\\tomsl\\Zotero\\storage\\6KJNW9HW\\Chen et al. - 2018 - Top-K Off-Policy Correction for a REINFORCE Recomm.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\tomsl\\Zotero\\storage\\25UBFSGS\\1812.html:text/html}
}

