	keywords = {Computer Science - Computer Vision and Pattern Recognition},
	file = {arXiv\:1607.08221 PDF:C\:\\Users\\tomsl\\Zotero\\storage\\2T4AF5PX\\Guo et al. - 2016 - MS-Celeb-1M A Dataset and Benchmark for Large-Sca.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\tomsl\\Zotero\\storage\\ZE474P7C\\1607.html:text/html}
}

